{
  "name": "task-rocket",
  "version": "1.0.0",
  "description": "Ondemand platform for both job seekers or individual head hunters",
  "main": "index.js",
  "scripts": {
    "test": "npm run test",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate-webpack:dev": "webpack-validator webpack.config.js && NODE_ENV=development",
    "validate-webpack:prod": "webpack-validator webpack.config.js && NODE_ENV=production",
    "clean-dist": "rimraf dist",
    "clean-and-copy": "npm run clean-dist && mkdir dist && cp index.html dist",
    "prestart": "npm run clean-and-copy",
    "start": "node server.js",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack && NODE_ENV=development",
    "build:prod": "webpack && NODE_ENV=production -p",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SunyoungKim508/task-rocket.git"
  },
  "author": "Sunyoung Kim (sunyoungkim508@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SunyoungKim508/task-rocket/issues"
  },
  "homepage": "https://github.com/SunyoungKim508/task-rocket#readme",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "react": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.14.3",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-safe-parser": "^2.0.0",
    "precss": "^1.4.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-validator": "^2.2.8"
  }
}
